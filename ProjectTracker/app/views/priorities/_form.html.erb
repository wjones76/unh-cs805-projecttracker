<%= form_for(@priority) do |f| %>
    <% if @priority.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@priority.errors.count, "error") %> prohibited this priority from being saved:</h2>

          <ul>
            <% @priority.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="row">
      <div class="col-sm-2">
        <%= f.label :id %>
      </div>
      <div class="col-sm-10">
        <%= f.text_field :id %>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-2">
        <%= f.label :priority %>
      </div>
      <div class="col-sm-10">
        <%= f.text_field :priority %>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-2">
      </div>
      <div class="col-sm-10">
        <div class="actions">
          <%= f.submit %>
        </div>
      </div>
    </div>
<% end %>
