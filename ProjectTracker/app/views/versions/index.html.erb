<h1>Versions</h1>

<table class="defaultTable">
  <tr>
    <th>Id</th>
    <th>Version name</th>
    <th>Project</th>
    <th></th>
  </tr>

  <% @versions.each do |version| %>
      <tr>
        <td><%= version.id %></td>
        <td><%= link_to version.version_name, version %></td>
        <td><%= version.project_id %></td>
        <td><%= link_to 'Edit', edit_version_path(version) %>
          | <%= link_to 'Destroy', version, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
</table>

<br/>

<%= link_to 'New Version', new_version_path %>
